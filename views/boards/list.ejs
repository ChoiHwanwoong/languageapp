                                                <th width="10%">수정일시</th>
                                            </tr>
                                        </thead>
                                        <tbody class="hoverTblBody">

                                            <% for(var i =boardList.length-1; i > -1;i--){ %>

                                                <tr>
                                                    <td><a href="/boards/modify/<%=boardList[i].article_idx%>"><%=boardList[i].article_idx%></a></td>
